{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kirikaeshi"
		},
		"is_blob_covidsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'is_blob_covidsa'"
		},
		"ls_ablob_coviddl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ablob_coviddl'"
		},
		"ls_ablob_coviddl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shinchan.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kirikaeshi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"is_blob_covidsa_connectionString": {
						"value": "[parameters('is_blob_covidsa_connectionString')]"
					},
					"ls_ablob_coviddl_accountKey": {
						"value": "[parameters('ls_ablob_coviddl_accountKey')]"
					},
					"ls_ablob_coviddl_properties_typeProperties_url": {
						"value": "[parameters('ls_ablob_coviddl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}